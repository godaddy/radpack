{
  "name": "@radpack/server",
  "version": "1.0.0",
  "description": "Rapid Application Development Packaging Server Runtime",
  "main": "./lib/index.cjs.js",
  "module": "./lib/index.esm.js",
  "exports": {
    ".": {
      "import": "./lib/index.esm.js",
      "require": "./lib/index.cjs.js"
    },
    "./middleware": {
      "import": "./lib/middleware.esm.js",
      "require": "./lib/middleware.cjs.js"
    }
  },
  "files": [
    "lib/"
  ],
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:godaddy/radpack.git",
    "directory": "packages/server"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@radpack/core": "^1"
  },
  "dependencies": {
    "glob": "^7.1.6",
    "serve-static": "^1.14.1"
  },
  "devDependencies": {
    "@radpack/core": "^1.0.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.47.0"
  },
  "license": "MIT"
}
