{
  "name": "@radpack/server",
  "version": "1.1.2",
  "description": "Rapid Application Development Packaging Server Runtime",
  "main": "./lib/index.cjs.js",
  "module": "./lib/index.esm.js",
  "exports": {
    ".": {
      "import": "./lib/index.esm.js",
      "require": "./lib/index.cjs.js"
    },
    "./middleware": {
      "import": "./lib/middleware.esm.js",
      "require": "./lib/middleware.cjs.js"
    }
  },
  "files": [
    "lib/",
    "middleware/"
  ],
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "rollup -c"
  },
  "homepage": "https://github.com/godaddy/radpack/tree/main/packages/server#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/godaddy/radpack.git",
    "directory": "packages/server"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@radpack/core": "^1"
  },
  "dependencies": {
    "glob": "^7.2.3",
    "serve-static": "^1.15.0"
  },
  "devDependencies": {
    "@radpack/core": "^1.0.4",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.78.1"
  },
  "license": "MIT"
}
