{
  "root": true,
  "extends": "godaddy",
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "babelOptions": {
      "configFile": "./configs/babel/node.js"
    }
  },
  "rules": {
    "eqeqeq": [
      "error",
      "smart"
    ],
    "max-params": [
      "warn",
      5
    ],
    "no-console": "error",
    "no-new-func": "off",
    "no-process-env": "off",
    "no-return-assign": "off",
    "no-sync": "off"
  },
  "globals": {
    "radpack": true,
    "globalThis": false
  }
}
